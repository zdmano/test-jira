{
  "LABEL": {
    "name": "PR title needs formatting",
    "color": "EEEEEE"
  },
  "CHECKS": {
    "prefixes": [
      "Bump"
    ],
    "regexp": "^(feat|fix|docs|style|refactor|perf|test|chore|BREAKING CHANGE)(\\(([a-zA-Z0-9 \\-]+)\\))?:[a-zA-Z0-9 \\-()]+(\\)?([A-Z][A-Z0-9]+-[0-9]+)\\)?[\\w ]*){1}$",
    "ignoreLabels": [
      "skip-pr-title-check"
    ]
  }
}